{
  "name": "Browser WebSocket Client",
  "manifest_version": 2,
  "version": "1.2.0",
  "description": "Connect to a WebSocket server to send and receive raw JSON messages.",
  "icons": {
    "16": "icon_016.png",
    "32": "icon_032.png",
    "48": "icon_048.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_032.png",
    "default_title": "Browser WebSocket Client"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
